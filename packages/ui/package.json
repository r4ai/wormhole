{
  "name": "@wormhole/ui",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r4ai/wormhole.git"
  },
  "license": "MIT",
  "author": {
    "name": "Rai"
  },
  "type": "module",
  "files": ["dist", "src", "styled-system", "package.json", "README.md"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "solid": "./dist/index.jsx",
      "default": "./dist/index.js"
    },
    "./preset": {
      "types": "./dist/preset.d.ts",
      "default": "./dist/preset.js"
    },
    "./style.css": "./dist/style.css",
    "./*": {
      "types": "./dist/*/index.d.ts",
      "solid": "./dist/*/index.jsx",
      "default": "./dist/*/index.js"
    }
  },
  "scripts": {
    "prepare": "panda codegen",
    "build": "tsup && panda cssgen -o dist/style.css",
    "dev": "tsup --watch"
  },
  "peerDependencies": {
    "solid-js": "^1.8.21"
  },
  "dependencies": {
    "@kobalte/core": "^0.13.5",
    "@wormhole/styled-system": "workspace:*"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.45.1",
    "solid-js": "1.8.21",
    "@types/bun": "1.1.8",
    "esbuild": "0.23.1",
    "esbuild-plugin-solid": "0.6.0",
    "tsup": "8.2.4",
    "typescript": "5.5.4"
  }
}
