{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["search", "settings"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "fs:default",
    "fs:scope-desktop-recursive",
    "fs:scope-appdata-recursive",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "store:allow-load",
    "store:allow-keys",
    "store:allow-values",
    "log:default"
  ]
}
